{
    "description": "Required fields to create a Currency",
    "required": true,
    "content": {
        "application/json": {
            "schema": {
                "type": "object",
                "required": [
                    "currencyCD",
                    "currencyName"
                ],
                "properties": {
                    "currencyCD": {
                        "description": "The ISO code of the currency.",
                        "type": "string"
                    },
                    "currencyName": {
                        "description": "The name of the currency",
                        "type": "string"
                    }
                },
                "example": {
                    "currencyCD": "AAA",
                    "currencyName": "TEST"
                }
            }
        }
    }
}